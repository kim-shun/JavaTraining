package example;

public class EmployeeManagement {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

	}

}

class Employee {
	int number;
	String name;

	Employee(int number, String name){
		this.number = number;
		this.name = name;
	}
	
	int getNum() {
		return number;
	}
	
	String getName() {
		return name;
	}
	
	int changeNum(int number) {
		this.number = number;
		return number;
	}
	
	String changeName(String name) {
		this.name = name;
		return name;
	}
}
