package example;

import java.util.ArrayList;
import java.util.Scanner;

public class EmployeeTest {

	public static void main(String[] args) {
		System.out.println("値を入力してください");
		@SuppressWarnings("resource")
		Scanner sc = new Scanner(System.in);

		int inputTimes = sc.nextInt();
		ArrayList<Employee> employeeList = new ArrayList<Employee>();
		for (int i = 0; i < inputTimes; i++) {
			String input = sc.next();
			if (input.equals("make")) {
				int num = sc.nextInt();
				String name = sc.next();
				Employee employee = new Employee(num, name);
				employeeList.add(employee);
			} else if (input.equals("getnum")) {
				int num = sc.nextInt();
				System.out.println(employeeList.get(num-1).getnum());
			}else if (input.equals("getname")) {
				int num = sc.nextInt();
				System.out.println(employeeList.get(num-1).name);
				
			}
		}

	}

}
private class Employee {
	int number;
	String name;
	static Employee(int number, String name){
		this.number = number;
		this.name = name;
	}

	int getnum(){
		return number;
	}
	String getname(){
		return name;
	}
}
